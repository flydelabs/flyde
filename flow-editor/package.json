{
  "name": "@flyde/flow-editor",
  "version": "1.0.0-alpha.12",
  "description": "",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "test": "mocha src/**/*.spec.tsx --require ts-node/register --require tsconfig-paths/register",
    "dev:ts": "tsc -p . -w --incremental",
    "dev": "concurrently \"tsc -p . -w --incremental\" \"tsc-alias -w\" \"tailwindcss -i ./src/styles/tailwind-entry.css -o ./dist/styles/tailwind.scss --watch\"",
    "__publish": "npm version patch && npm publish",
    "prebuild": "pnpm run bundle-types",
    "build": "rm -rf dist && tsc -p . && pnpm run build:tw && tsc-alias",
    "build:tw": "tailwindcss -i ./src/styles/tailwind-entry.css -o ./dist/styles/tailwind.scss --minify",
    "start": "npm run watch",
    "storybook": "start-storybook",
    "bundle-types:core": "dts-bundle --name @flyde/core --main node_modules/@flyde/core/dist/index.d.ts --out ../../../../src/types/@flyde-core.d.ts",
    "bundle-types:stringify": "tsx scripts/generate-types-file.ts",
    "bundle-types:clean-dts": "rm -rf src/types/@flyde-core.d.ts",
    "bundle-types": "pnpm run bundle-types:core && pnpm run bundle-types:stringify && pnpm run bundle-types:clean-dts"
  },
  "author": "",
  "license": "agpl-3.0",
  "dependencies": {
    "@flyde/core": "workspace:*",
    "@flyde/remote-debugger": "workspace:*",
    "@flyde/stdlib": "workspace:*",
    "@flyde/ui": "workspace:*",
