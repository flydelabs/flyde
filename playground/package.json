{
  "private": true,
  "name": "@flyde/playground",
  "scripts": {
    "dev": "pnpm run bundle-types && pnpm run build:inline-macros && npx open-cli http://localhost:3030 && next dev --port 3030",
    "start": "next start",
    "build": "pnpm run bundle-types && pnpm run build:inline-macros && next build",
    "lint": "next lint",
    "update:flyde": "npm update --scope @flyde",
    "types": "npx --yes supabase gen types typescript --project-id rfvvslzvfkyaoyngcjti --schema public -o types/supabase.ts",
    "bundle-types": "pnpm run bundle-types:core && pnpm run bundle-types:runtime",
    "bundle-types:core": "dts-bundle --name @flyde/core --main node_modules/@flyde/core/dist/index.d.ts --out types/@flyde-core.d.ts",
    "bundle-types:runtime": "dts-bundle --name @flyde/runtime --main node_modules/@flyde/runtime/dist/index.d.ts --out types/@flyde-runtime.d.ts",
    "build:inline-macros": "tsx scripts/build-inline-macros.mts",
    "link-all": "npm link @flyde/core @flyde/flow-editor @flyde/stdlib @flyde/runtime @flyde/resolver"
  },
  "dependencies": {
    "@codemirror/lang-javascript": "^6.1.9",
    "@flyde/core": "workspace:*",
    "@flyde/flow-editor": "workspace:*",
    "@flyde/remote-debugger": "workspace:*",
    "@flyde/resolver": "workspace:*",
    "@flyde/runtime": "workspace:*",
    "@flyde/stdlib": "workspace:*",
    "@monaco-editor/react": "^4.5.2",
    "@react-hook/size": "^2.1.2",
    "@supabase/auth-helpers-nextjs": "latest",
    "@supabase/supabase-js": "^2.39.0",
    "@types/file-saver": "^2.0.5",
    "@types/nprogress": "^0.2.3",
    "@types/react-resizable": "^3.0.7",
    "@uidotdev/usehooks": "^2.1.0",
    "@uiw/react-codemirror": "^4.21.9",
    "autoprefixer": "10.4.14",
    "dts-bundle": "^0.7.3",
    "file-saver": "^2.0.5",
    "jszip": "^3.10.1",
    "next": "^13.4.20-canary.5",
    "nextjs-progressbar": "^0.0.16",
    "postcss": "8.4.24",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-resizable": "^3.0.5",
    "react-simple-toasts": "^5.9.0",
    "react-tiny-popover": "^7.2.4",
    "react-tooltip": "^5.21.4",
    "sass": "^1.66.1",
    "tailwindcss": "3.3.2",
    "typescript": "5.5.4"
  },
  "devDependencies": {
    "@types/node": "20.3.1",
    "@types/react": "18.2.12",
    "@types/react-dom": "18.2.5",
    "encoding": "^0.1.13",
    "eslint": "8.50.0",
    "eslint-config-next": "13.5.2",
    "open-cli": "^7.2.0",
    "raw-loader": "^4.0.2",
    "tsx": "^4.7.1"
  }
}